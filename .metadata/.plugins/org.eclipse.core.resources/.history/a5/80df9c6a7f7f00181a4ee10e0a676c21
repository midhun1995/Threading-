package com.hexa.demo;

class Exam {
String name;
public void setName(String newName) {
name = newName;
System.out.println(name);
}
}
class ObjectLife1 {
public static void main(String args[]) {
Exam myExam = new Exam();
myExam.setName("OCA Java Programmer 1");
myExam = new Exam();
myExam = null;
myExam.setName("PHP");

}
}